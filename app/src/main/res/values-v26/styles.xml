<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppButton" parent="Widget.AppCompat.Button">
        <item name="android:backgroundTint">#12B956</item>
        <item name="android:background">@drawable/button_shape</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/roboto_condensed_medium</item>
    </style>
    <style name="CustomNavigationBarActiveIndicator">
        <item name="android:background">@drawable/bottom_nav_active_indicator</item>
    </style>

    <style name="RoundedIndicator" parent="ShapeAppearance.Material3.SmallComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
</resources>